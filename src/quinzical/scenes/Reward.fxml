<AnchorPane prefHeight="256.0" prefWidth="406.0" style="-fx-background-color: #121F26;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="quinzical.scenes.RewardController">
